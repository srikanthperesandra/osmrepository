# Relations defining route as road
db.relations.find({"$where":"this.metaData.type==\"route\" && this.metaData.route==\"road\""})

# Routes of Zollernalbkreis 
db.relations.find({"$where":"this.metaData.type==\"route\" && this.metaData.route==\"road\" && this.metaData.operator==\"Zollernalbkreis\""})


